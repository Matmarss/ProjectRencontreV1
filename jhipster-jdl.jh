entity Personne {
	nom String required,
    prenom String required,
    mail String required,
    genre Genre required,
    motDePasse String required,
    naissance String required,
    listCarac Caracteristiques
}

enum Caracteristiques {
	JOYEUX, AMICAL, RIEUR, CHANCEUX
}
enum Genre {
	MASCULIN, FEMININ
}




